Map {
  background-color: #b8dee6;
}

#countries {
  ::outline {
    line-color: #85c5d3;
    line-width: 2;
    line-join: round;
  }
  polygon-fill: #fff;
}

#lgashapesjoined {
  line-color:#75d85e;
  line-width:5.5;
  polygon-opacity:1;
  polygon-fill:#168e8c;
  
  text-name: [lgashort];
  text-face-name:'Arial Bold';
  text-fill:#FFFFFF;
}